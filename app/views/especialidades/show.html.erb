<p style="color: green"><%= notice %></p>

<h2>Especialidades<%=@especialidad.Almunos.count %> </h2>


<table class="table table-striped table-hover">
 <thead>
    <tr>
      <th>id</th>
      <th>Especialidad</th>
      <th>Carrera</th>
      <th>Usuario</th>
      <th>No_Control</th>
      <th>Semestre</th>
      <th>Nombre</th>
      <th>Apellido_P</th>
      <th>Apellido_M</th>
      <th>Ver especialidad</th>
      <th>Editar especialidad</th>

      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @especialidad.Almunos.each do |e| %>
    <tr>
      <td><%= e[:id] %></td>
      <td><%= e.Especialidad[:nombre_especialidad] %></td>
      <td><%= e[:carrera_id] %></td>
      <td><%= e[:user_id] %></td>
      <td><%= e[:no_control] %></td>
      <td><%= e[:semestre] %></td>
      <td><%= e[:nombre] %></td>
      <td><%= e[:apellido_p] %></td>
      <td><%= e[:apellido_m] %></td>
      <td><%= link_to "Regresar a especialidades", especialidades_path %></td>
      <td> <button type="button" class="btn btn-warning"> <%= link_to "Edit this especialidad", edit_especialidad_path(@especialidad) %> </td>
      <td><%= button_to "Destroy this especialidad", @especialidad, method: :delete %></td>
    </tr>
  <% end %>
  </tbody>
</table>




